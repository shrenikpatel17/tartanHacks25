{
    "manifest_version": 3,
    "name": "Text Extractor",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting",
      "tabs",
      "storage"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*",
      "https://api.openai.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["whale.png","menu.png","fish.png","chat.png"],
        "matches": ["<all_urls>"]
      }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html"
    },
    "options_page": "options.html"
  }
  